StopAllSoundsBtn.MouseButton1Click:Connect(function()
	for _, music in pairs(game.Workspace:GetDescendants()) do
		if music:IsA("Sound") then
			music:Destroy()
		end
	end
	for _, music in pairs(game.SoundService:GetDescendants()) do
		if music:IsA("Sound") then
			music:Destroy()
		end
	end
end)
   end,
